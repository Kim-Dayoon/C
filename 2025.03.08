#include <stdio.h>
int main()
{
	int ar[100];
	int n=0;
	int f;
	int t;
	scanf("%d",&f);
	for(int i=0;i<f;i++)
	{
		scanf("%d",&ar[i]);
	}
	scanf("%d",&t);
	for(int i=0;i<f;i++)
	{
		if(ar[i] == t)
		{
			n+=1;
		}
	}
	printf("%d ",n);
}
