#include <stdio.h>
int main()
{
	int g;
	int c;
	int n;
	printf("학년  입력하기: ");
	scanf("%d",&g);
	printf("반 입력하기: ");
	scanf("%d",&c);
	printf("번호 입력하기: ");
	scanf("%d",&n);
	if(10<=n)
	{
		printf("%d%d%d",g,c,n);
	}
	else
	{
		printf("%d%d0%d",g,c,n);
	}
	
}
