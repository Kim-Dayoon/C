#include<stdio.h>
void divisor(int n)
{
	int sum=0;
	for(int i=1;i<=n;i++)
	{
		if(n%i==0)
		{
			printf("%d ",i);
			sum+=i;
		}
		
		
	}
	printf("%d",sum);
}
int main()
{
	int n;
	scanf("%d",&n);
	divisor(n);
}
