#include<stdio.h>
void add(int* num1,int* num2)
{
	*num1=*num1+10;
	*num2=*num2+10;	
}
int main()
{
	int num1=5;
	int num2=10;
	add(&num1,&num2);
	printf("num1=%d\n",num1);
	printf("num2=%d\n",num2);
}
