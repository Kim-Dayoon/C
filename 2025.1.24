#include <stdio.h>
int main()
{
	int n;
	scanf("%d",&n);
	
	for (int i=0;i<n;i++)
	{
		int a,b;
		scanf("%d %d",&a,&b);
		printf("\n출력 결과:%d\n",a+b);
	}
}
